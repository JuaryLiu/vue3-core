{"code":"// 创建vnode\n// createVnode = h函数\nimport { isArray, isObject, isString } from \"@vue/shared\";\nexport const createVnode = (type, props, children = null) => {\n    // 区分 是组件还是元素\n    let shapeFlag = isString(type) ? 1 /* ShapeFlags.ELEMENT */ : isObject(type) ? 4 /* ShapeFlags.STATEFUL_COMPONENT */ : 0;\n    const vnode = {\n        _v_isVnode: true,\n        type,\n        props,\n        children,\n        key: props && props.key,\n        el: null,\n        shapeFlag\n    };\n    //children 标识\n    normalizeChildren(vnode, children);\n    return vnode;\n};\nfunction normalizeChildren(vnode, children) {\n    let type = 0;\n    if (children == null) {\n        children = null;\n    }\n    else if (isArray(children)) { // 数组\n        type = 16 /* ShapeFlags.ARRAY_CHILDREN */;\n    }\n    else { // 文本\n        type = 8 /* ShapeFlags.TEXT_CHILDREN */;\n    }\n    vnode.shapeFlag = vnode.shapeFlag | type; // 合并标识\n}\n//# sourceMappingURL=vnode.js.map","references":["C:/Users/JuaryLiu/Desktop/vue3-core/packages/shared/src/index.ts"],"map":"{\"version\":3,\"file\":\"vnode.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../packages/runtime-core/src/vnode.ts\"],\"names\":[],\"mappings\":\"AAAA,UAAU;AACV,oBAAoB;AACpB,OAAO,EAAc,OAAO,EAAE,QAAQ,EAAE,QAAQ,EAAE,MAAM,aAAa,CAAA;AACrE,MAAM,CAAC,MAAM,WAAW,GAAG,CAAC,IAAQ,EAAC,KAAS,EAAC,QAAQ,GAAE,IAAI,EAAE,EAAE;IAC7D,aAAa;IACb,IAAI,SAAS,GAAG,QAAQ,CAAC,IAAI,CAAC,CAAA,CAAC,4BAAkB,CAAC,CAAA,QAAQ,CAAC,IAAI,CAAC,CAAA,CAAC,uCAA6B,CAAC,CAAA,CAAC,CAAA;IAChG,MAAM,KAAK,GAAG;QACV,UAAU,EAAC,IAAI;QACf,IAAI;QACJ,KAAK;QACL,QAAQ;QACR,GAAG,EAAC,KAAK,IAAI,KAAK,CAAC,GAAG;QACtB,EAAE,EAAC,IAAI;QACP,SAAS;KACZ,CAAA;IACD,aAAa;IACb,iBAAiB,CAAC,KAAK,EAAC,QAAQ,CAAC,CAAA;IACjC,OAAO,KAAK,CAAA;AAChB,CAAC,CAAA;AAED,SAAS,iBAAiB,CAAC,KAAS,EAAC,QAAgB;IACjD,IAAI,IAAI,GAAG,CAAC,CAAA;IACZ,IAAG,QAAQ,IAAI,IAAI,EAAE;QACjB,QAAQ,GAAG,IAAI,CAAA;KAClB;SAAK,IAAG,OAAO,CAAC,QAAQ,CAAC,EAAE,EAAE,KAAK;QAC/B,IAAI,qCAA4B,CAAA;KACnC;SAAM,EAAE,KAAK;QACV,IAAI,mCAA0B,CAAA;K