{"code":"// 操作属性\nimport { patchClass } from './modules/class';\nimport { patchStyle } from './modules/style';\nimport { pathAttr } from './modules/attr';\nimport { pathEvent } from './modules/event';\nexport const patchProp = (el, key, prevValue, nextValue) => {\n    switch (key) {\n        case 'class':\n            patchClass(el, prevValue);\n            break;\n        case 'style':\n            patchStyle(el, prevValue, nextValue);\n            break;\n        default:\n            if (/^on[^a-z]/.test(key)) { // 是不是事件\n                pathEvent(el, key, nextValue);\n            }\n            else {\n                pathAttr(el, key, nextValue);\n            }\n            break;\n    }\n};\n//# sourceMappingURL=patchProp.js.map","references":["C:/Users/JuaryLiu/Desktop/vue3-core/packages/runtime-dom/src/modules/class.ts","C:/Users/JuaryLiu/Desktop/vue3-core/packages/runtime-dom/src/modules/style.ts","C:/Users/JuaryLiu/Desktop/vue3-core/packages/runtime-dom/src/modules/attr.ts","C:/Users/JuaryLiu/Desktop/vue3-core/packages/runtime-dom/src/modules/event.ts"],"map":"{\"version\":3,\"file\":\"patchProp.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../packages/runtime-dom/src/patchProp.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO;AACP,OAAO,EAAC,UAAU,EAAC,MAAM,iBAAiB,CAAA;AAC1C,OAAO,EAAE,UAAU,EAAE,MAAM,iBAAiB,CAAC;AAC7C,OAAO,EAAE,QAAQ,EAAE,MAAM,gBAAgB,CAAC;AAC1C,OAAO,EAAE,SAAS,EAAE,MAAM,iBAAiB,CAAC;AAC5C,MAAM,CAAC,MAAM,SAAS,GAAG,CAAC,EAAU,EAAC,GAAO,EAAC,SAAa,EAAC,SAAa,EAAE,EAAE;IACxE,QAAO,GAAG,EAAE;QACR,KAAK,OAAO;YACR,UAAU,CAAC,EAAE,EAAC,SAAS,CAAC,CAAA;YAC5B,MAAM;QACN,KAAK,OAAO;YACR,UAAU,CAAC,EAAE,EAAC,SAAS,EAAC,SAAS,CAAC,CAAA;YACtC,MAAM;QACN;YACI,IAAG,WAAW,CAAC,IAAI,CAAC,GAAG,CAAC,EAAE,EAAE,QAAQ;gBAChC,SAAS,CAAC,EAAE,EAAC,GAAG,EAAC,SAAS,CAAC,CAAA;aAC9B;iBAAM;gBACH,QAAQ,CAAC,EAAE,EAAC,GAAG,EAAC,SAAS,CAAC,CAAA;aAC7B;YACD,MAAM;KACb;AACL,CAAC,CAAA\"}"}
